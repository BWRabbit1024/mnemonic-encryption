{
  "expo": {
    "name": "Mnemonic Encryption",
    "slug": "secure-mnemonic-encrypt",
    "version": "1.1.1",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "icon": "./assets/icon.png",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#1e90ff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.securemnemonic.encrypt"
    },
    "android": {
      "package": "com.securemnemonic.encrypt",
      "versionCode": 2,
      "permissions": [],
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon_foreground.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-asset"
    ],
    "extra": {
      "eas": {
        "projectId": "dc138022-8e57-4f4a-b15b-a858eccef86a"
      }
    },
    "owner": "bwrabbit1024"
  }
}
